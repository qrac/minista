{
  "name": "minista-monorepo",
  "version": "4.0.0-alpha.7",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "homepage": "https://minista.qranoko.jp",
  "keywords": [
    "static-site-generator",
    "ssg",
    "coding",
    "react",
    "jsx",
    "tsx",
    "typescript",
    "esbuild",
    "vite",
    "japanese",
    "minista"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "build": "run-s build:utils build:head && run-p build:main build:ssg build:enhance build:bundle build:mdx build:story build:beautify build:archive",
    "build-src": "run-p build-src:*",
    "build:main": "cd ./packages/minista && npm run build",
    "build-src:main": "cd ./packages/minista && npm run build-src",
    "build:ssg": "cd ./packages/plugin-ssg && npm run build",
    "build-src:ssg": "cd ./packages/plugin-ssg && npm run build-src",
    "build:enhance": "cd ./packages/plugin-enhance && npm run build",
    "build-src:enhance": "cd ./packages/plugin-enhance && npm run build-src",
    "build:mdx": "cd ./packages/plugin-mdx && npm run build",
    "build-src:mdx": "cd ./packages/plugin-mdx && npm run build-src",
    "build:bundle": "cd ./packages/plugin-bundle && npm run build",
    "build-src:bundle": "cd ./packages/plugin-bundle && npm run build-src",
    "build:hydrate": "cd ./packages/plugin-hydrate && npm run build",
    "build-src:hydrate": "cd ./packages/plugin-hydrate && npm run build-src",
    "build:story": "cd ./packages/plugin-story && npm run build",
    "build-src:story": "cd ./packages/plugin-story && npm run build-src",
    "build:beautify": "cd ./packages/plugin-beautify && npm run build",
    "build-src:beautify": "cd ./packages/plugin-beautify && npm run build-src",
    "build:archive": "cd ./packages/plugin-archive && npm run build",
    "build-src:archive": "cd ./packages/plugin-archive && npm run build-src",
    "build:head": "cd ./packages/shared-head && npm run build",
    "build-src:head": "cd ./packages/shared-head && npm run build-src",
    "build:utils": "cd ./packages/shared-utils && npm run build",
    "build-src:utils": "cd ./packages/shared-utils && npm run build-src",
    "test": "vitest run",
    "test:w": "vitest watch",
    "play": "npx minista ./playground/_default",
    "play-build": "npx minista build ./playground/_default",
    "play-preview": "npx minista preview ./playground/_default",
    "play:enhance": "npx minista ./playground/enhance",
    "play-build:enhance": "npx minista build ./playground/enhance",
    "play-preview:enhance": "npx minista preview ./playground/enhance",
    "play:mdx": "npx minista ./playground/mdx",
    "play-build:mdx": "npx minista build ./playground/mdx",
    "play-preview:mdx": "npx minista preview ./playground/mdx",
    "play:bundle": "npx minista ./playground/bundle",
    "play-build:bundle": "npx minista build ./playground/bundle",
    "play-preview:bundle": "npx minista preview ./playground/bundle",
    "play:hydrate": "npx minista ./playground/hydrate",
    "play-build:hydrate": "npx minista build ./playground/hydrate",
    "play-preview:hydrate": "npx minista preview ./playground/hydrate",
    "play:story": "npx minista ./playground/story",
    "play:storyapp": "npx minista ./playground/storyapp",
    "play-build:story": "npx minista build ./playground/story",
    "play-preview:story": "npx minista preview ./playground/story",
    "play:beautify": "npx minista ./playground/beautify",
    "play-build:beautify": "npx minista build ./playground/beautify",
    "play-preview:beautify": "npx minista preview ./playground/beautify",
    "play:archive": "npx minista ./playground/archive",
    "play-build:archive": "npx minista build ./playground/archive",
    "play-preview:archive": "npx minista preview ./playground/archive",
    "deno": "deno run --allow-read --allow-net --allow-env --allow-run --import-map=import_map.json ./packages/minista/bin/minista.js ./playground/_default",
    "deno-build": "deno run --allow-read --allow-net --allow-env --allow-run --import-map=import_map.json ./packages/minista/bin/minista.js build ./playground/_default",
    "bun": "bun minista ./playground/_default",
    "bun-build": "bun minista build ./playground/_default",
    "up": "npx npm-check-updates --deep --reject minista-shared-head,minista-shared-utils",
    "up:u": "npx npm-check-updates --deep -u --reject minista-shared-head,minista-shared-utils",
    "rev": "npx npm-replace-versions --deps minista-shared-head,minista-shared-utils --ignore **/node_modules",
    "release": "run-s release:*",
    "release:main": "cd ./packages/minista && npm publish",
    "release-next": "run-s release-next:*",
    "release-next:main": "cd ./packages/minista && npm publish --tag=next",
    "release-next:ssg": "cd ./packages/plugin-ssg && npm publish --tag=next",
    "release-next:enhance": "cd ./packages/plugin-enhance && npm publish --tag=next",
    "release-next:mdx": "cd ./packages/plugin-mdx && npm publish --tag=next",
    "release-next:bundle": "cd ./packages/plugin-bundle && npm publish --tag=next",
    "release-next:story": "cd ./packages/plugin-story && npm publish --tag=next",
    "release-next:beautify": "cd ./packages/plugin-beautify && npm publish --tag=next",
    "release-next:archive": "cd ./packages/plugin-archive && npm publish --tag=next",
    "release-next:head": "cd ./packages/shared-head && npm publish --tag=next",
    "release-next:utils": "cd ./packages/shared-utils && npm publish --tag=next",
    "clean": "run-p clean:*",
    "clean:main": "cd ./packages/minista && npm run clean",
    "clean:create": "cd ./packages/create-minista && npm run clean",
    "clean:ssg": "cd ./packages/plugin-ssg && npm run clean",
    "clean:enhance": "cd ./packages/plugin-enhance && npm run clean",
    "clean:mdx": "cd ./packages/plugin-mdx && npm run clean",
    "clean:bundle": "cd ./packages/plugin-bundle && npm run clean",
    "clean:story": "cd ./packages/plugin-story && npm run clean",
    "clean:beautify": "cd ./packages/plugin-beautify && npm run clean",
    "clean:archive": "cd ./packages/plugin-archive && npm run clean",
    "clean:head": "cd ./packages/shared-head && npm run clean",
    "clean:utils": "cd ./packages/shared-utils && npm run clean"
  },
  "workspaces": [
    "packages/*",
    "playground/*"
  ],
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^20.14.10",
    "@types/picomatch": "^3.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.23.0",
    "npm-run-all2": "^6.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.1",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^2.0.2"
  }
}
