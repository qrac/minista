{
  "name": "minista",
  "version": "4.0.0-alpha.9",
  "type": "module",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "bin": {
    "minista": "./bin/minista.js"
  },
  "types": "dist/types/@types/node.d.ts",
  "main": "dist/node/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/@types/node.d.ts",
      "default": "./dist/node/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "files": [
    "bin",
    "dist",
    "client.d.ts"
  ],
  "license": "MIT",
  "homepage": "https://minista.qranoko.jp",
  "repository": {
    "type": "git",
    "url": "https://github.com/qrac/minista",
    "directory": "packages/minista"
  },
  "bugs": {
    "url": "https://github.com/qrac/minista/issues"
  },
  "keywords": [
    "static-site-generator",
    "ssg",
    "coding",
    "react",
    "jsx",
    "tsx",
    "typescript",
    "esbuild",
    "vite",
    "japanese",
    "minista"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "build": "npm run clean && npm run build-src && npm run build-type",
    "build-src": "esbuild ./src/node/*.ts --outbase=src --outdir=dist",
    "build-type": "tsc",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "dependencies": {
    "minista-shared-utils": "^4.0.0-alpha.9"
  }
}
